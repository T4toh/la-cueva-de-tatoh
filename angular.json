{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "perfil-personal": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/perfil-personal",
      "sourceRoot": "projects/perfil-personal/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/perfil-personal/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/perfil-personal/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/perfil-personal/public"
              },
              "_redirects"
            ],
            "styles": [
              "projects/perfil-personal/src/styles.scss"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "perfil-personal:build:production"
            },
            "development": {
              "buildTarget": "perfil-personal:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/perfil-personal/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/perfil-personal/public"
              },
              "_redirects"
            ],
            "styles": [
              "projects/perfil-personal/src/styles.scss"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/perfil-personal/**/*.ts",
              "projects/perfil-personal/**/*.html"
            ],
            "eslintConfig": "projects/perfil-personal/eslint.config.js"
          }
        }
      }
    },
    "componentes": {
      "projectType": "library",
      "root": "projects/componentes",
      "sourceRoot": "projects/componentes/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/componentes/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/componentes/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/componentes/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/componentes/**/*.ts",
              "projects/componentes/**/*.html"
            ],
            "eslintConfig": "projects/componentes/eslint.config.js"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "ec4452f5-fb92-4942-86e2-0edb7e4d8d04",
    "schematicCollections": [
      "angular-eslint"
    ]
  }
}
